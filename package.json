    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@netlify/functions": "^0.10.0",
    "@prisma/client": "^3.9.1",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "leaflet": "^1.7.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.5",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/typography": "^0.5.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.6",
    "prisma": "^3.9.1",
    "tailwindcss": "^3.0.18"
  },
  "engines": {
    "node": ">=14"
